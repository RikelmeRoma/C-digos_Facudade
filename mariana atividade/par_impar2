package Mariana_atv1;


public class Resultado{
    public static void main(String[] args) {
        Num num = new Num(14); 

        if (num.Par()) {
            System.out.println(num.getValor() + " é par.");
        } else if (num.Impar()) {
            System.out.println(num.getValor() + " é ímpar.");
        }
    }
}
