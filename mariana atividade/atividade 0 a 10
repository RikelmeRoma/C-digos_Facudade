import java.util.Scanner;


class NumeroContador {
    private int[] numeros;
    private int numeroEspecifico; 
    private int contagem;

  
    public NumeroContador() {
        numeros = new int[10];
        contagem = 0;
    }

    
    public void setNumeros(int[] numeros) {
        this.numeros = numeros;
    }

    
    public int[] getNumeros() {
        return numeros;
    }


    public void setNumeroEspecifico(int numeroEspecifico) {
        this.numeroEspecifico = numeroEspecifico;
    }

    
    public int getNumeroEspecifico() {
        return numeroEspecifico;
    }



    public int getContagem() {
        return contagem;
    }
}
