package Mariana_atv1;


public class Num {
    private int valor; 

    public Num(int valor) {
        this.valor = valor;
    }

    public int getValor() {
        return valor;
    }

    public void setValor(int valor) {
        this.valor = valor;
    }

    public boolean Par() {
        return valor % 2 == 0;
    }

    public boolean Impar() {
        return valor % 2 != 0;
    }
}
